
.content {
}
.avg {
  width: 35px;
  height: 35px;
  border-radius: 50%;
  background: #19B888;
}
.top1 {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-right: 100px;
}
.co2 {
  height: 160px;
  padding-top: 10px;
  padding-bottom: 20px;
  background: #F6F7F7;
  border-radius: 20px;
  margin: 14px auto;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.co3 {
  min-height: 80px;
  padding-top: 10px;
  padding-bottom: 20px;
  background: #F6F7F7;
  border-radius: 20px;
  margin: 14px auto;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  font-size: 12px;
}
.progres{
  border-top: 3px solid pink;
}
.co2-item{
  width: 42%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.food{
  width: 85%;
  margin: 14px auto;
}
.food_title{
  display: flex;
  justify-content: space-between;
}
.sandian{
  width: 60px;
  height: 30px;
  background: #173430;
  border-radius: 30px;
  color: #FFFFFF;
  text-align: center;
  font-size: 20px;
}
.clicktab{
  display: flex;
  justify-content: space-around;
  margin: 20px auto;
}
.tab-item{
  width: 25%;
  height: 25px;
  line-height: 25px;
  text-align: center;
  font-size: 12px;
  color: #FFFFFF;
  border-radius: 15px;
  background: #173430;
}
.big_bg{
  background: #ebf6d5;
  width: 100%;
  height: 300px;
  border-radius: 30px;
}
.bigcard{
  width: 100%;
  height: 300px;
  background: #ebf6d4;
  border-radius: 30px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.cardtitle{
  width: 120px;
  height: 50px;
  background: #FFFFFF;
  text-align: center;
  line-height: 50px;
  border-radius: 25px;
  font-size: 20px;
}
