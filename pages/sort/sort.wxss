.popup_overlay {
  position: fixed;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.8;
  opacity: .80;
  filter: alpha(opacity=88);
}
.popup_content {
  position: fixed;
  bottom: 60px;
  left: 38%;
  width: 90%;
  height: 340rpx;
  margin-left: -270rpx;
  margin-top: -270rpx;
  border: 10px solid white;
  background-color: white;
  border-radius: 30px;
  z-index: 1002;
  overflow: auto;
}
